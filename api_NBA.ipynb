    "## Lo primero es poner las tablas por Conferencia\n",
    "\n",
    "data = {'Pos':[],'TeamID': [], 'Team': [], 'Conference':[], 'Division':[], 'Wins': [], 'Losses': [], 'Percentage': [], 'HomeWins': [], 'HomeLosses': [], 'AwayWins': [], 'AwayLosses': [], 'LastTenWins': [], 'LastTenLosses': [], 'PointsPerGameFor': [], 'PointsPerGameAgainst': [], 'Diff': [], 'Streak': []}\n",
    "    \n",
    "\n",
    "for equipo in tabla:\n",
    "    \n",
    "    for info in data.keys():\n",
    "        if info == 'Team':\n",
    "            data['Team'].append(equipo['City'] + ' ' + equipo['Name'])\n",
    "        elif info == 'Diff':\n",
    "            data['Diff'].append(equipo['PointsPerGameFor'] - equipo['PointsPerGameAgainst'])\n",
    "        elif info == 'Streak':\n",
    "            data['Streak'].append(equipo['StreakDescription'])\n",
    "        elif info == 'Pos':\n",
    "            data['Pos'].append(None)\n",
    "        else:\n",
    "            data[info].append(equipo[info])\n",
    "        \n",
    "    \n",
    "    \n",
    "df_tabla = pd.DataFrame(data).sort_values(by=['Percentage', 'Diff', 'PointsPerGameFor'], ascending=False).reset_index(drop=True)\n",
    "\n",
    "for i in range(len(df_tabla.index)):\n",
    "    df_tabla['Pos'][i] = i + 1\n",
    "\n",
    "df_tabla.rename(columns={'Percentage': 'W/L %', 'HomeWins': 'H. W.', 'HomeLosses': 'H. L.', 'AwayWins': 'A. W.', 'AwayLosses': 'A. L.', 'LastTenWins': 'Last 10 W.', 'LastTenLosses': 'Last 10 L.', 'PointsPerGameFor': 'PPGF', 'PointsPerGameAgainst': 'PPGA'}, inplace=True)\n",
    "\n",
    "\n",
    "\n",
    "dfi.export(df_tabla, 'clasificacion_global.png', table_conversion='matplotlib')\n",
    "   \n",
    "    "
   ]
