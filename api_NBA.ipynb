    "\n",
    "if 'Dallas Mavericks' in df_partidos['Local'] or 'Dallas Mavericks' in df_partidos['Visitante']:\n",
    "    \n",
    "    if 'Dallas Mavericks' in df_partidos['Local'].values:\n",
    "        local = 'Dallas Mavericks'\n",
    "        visitante = df_partidos['Visitante'][list(df_partidos['Local']).index('Dallas Mavericks')]\n",
    "        resultado = df_partidos['Resultado'][list(df_partidos['Local']).index('Dallas Mavericks')]\n",
    "    else:\n",
    "        visitante = 'Dallas Mavericks'\n",
    "        local = df_partidos['Local'][list(df_partidos['Visitante']).index('Dallas Mavericks')]\n",
    "        resultado = df_partidos['Resultado'][list(df_partidos['Visitante']).index('Dallas Mavericks')]\n",
    "    \n",
    "    \n",
    "\n",
    "else:\n",
    "    for i in range(len(df_tabla.index)):\n",
    "        equipo = df_tabla['Team'][i]\n",
    "        \n",
    "        if equipo in df_partidos['Local'].values:\n",
    "            local = equipo\n",
    "            visitante = df_partidos['Visitante'][list(df_partidos['Local']).index(equipo)]\n",
    "            resultado = df_partidos['Resultado'][list(df_partidos['Local']).index(equipo)]\n",
    "            break\n",
    "        elif equipo in df_partidos['Visitante'].values:\n",
    "            \n",
    "            visitante = equipo\n",
    "            local = df_partidos['Local'][list(df_partidos['Visitante']).index(equipo)]\n",
    "            resultado = df_partidos['Resultado'][list(df_partidos['Visitante']).index(equipo)]\n",
    "            break\n",
    "    \n",
    "id_local = df_tabla[df_tabla['Team'] == local]['TeamID'].values[0]\n",
    "id_visitante = df_tabla[df_tabla['Team'] == visitante]['TeamID'].values[0]\n",
    "\n"
