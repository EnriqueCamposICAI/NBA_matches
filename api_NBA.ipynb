    "\n",
    "# Primero miramos los pronósticos y vemos los partidos que hay\n",
    "\n",
    "response = requests.get(\n",
    "        'https://www.sportytrader.es/pronosticos/baloncesto/usa/nba-306/')\n",
    "html = response.text\n",
    "\n",
    "soup = bs(html, 'html.parser')\n",
    "\n",
    "equipos = {'Local': [], 'Visitante': [], 'Resultado': []}\n",
    "\n",
    "resultados = {'1': [], 'X': [], '2': []}\n",
    "\n",
    "# Obtener los equipos\n",
    "\n",
    "data1 = soup.find_all(\n",
    "    'div', class_=\"w-1/2 text-center break-word p-1 dark:text-white\")\n",
    "\n",
    "for i in range(len(data1)):\n",
    "    if i % 2 == 0:\n",
    "        equipos['Local'].append(re.sub(r'\\n', \"\", data1[i].text))\n",
    "    else:\n",
    "        equipos['Visitante'].append(re.sub(r'\\n', '', data1[i].text))\n",
    "\n",
    "data_gan = soup.find_all(\n",
    "    'span', class_=\"flex justify-center items-center h-7 w-6 rounded-md font-semibold bg-primary-green text-white mx-1\")\n",
    "\n",
    "for i in range(len(data_gan)):\n",
    "    equipos['Resultado'].append(re.sub(r'\\n','',data_gan[i].text))\n",
    "\n",
    "df_partidos = pd.DataFrame(equipos)\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
